function scr_tower_planned_delete(field_x, field_y){
	
	var planned_before = scr_get_tower_planned(field_x, field_y); 
	if planned_before != -1 { 
		var p_before = scr_get_price(planned_before);  
	
		if planned_before == objCollector scr_set_price(objCollector, scr_get_price(objCollector) - 3)
	
		objPlayer.money += p_before;
		
		scr_plan_tower(field_x, field_y, 0);
	}

	
	
}